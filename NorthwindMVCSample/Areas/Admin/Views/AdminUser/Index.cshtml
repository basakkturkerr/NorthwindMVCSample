
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/SiteLayout.cshtml";
}

@model List<NorthwindMVCSample.Models.VM.AdminUserVM>

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">AdminUser List</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                AdminUser List
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Name</th>
                                <th>Surname</th>
                                <th>EMail</th>
                                <th>Password</th>
                                <th>LastLoginDate</th>
                                <th>Process</th>
                                <th>Detail</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                <td>@item.ID</td>
                                <td>@item.Name</td>
                                <td>@item.Surname</td>
                                <td>@item.EMail</td>
                                <td>@item.Password</td>
                                <td>@item.LastLoginDate</td>
                                <td><a href="/Admin/AdminUser/deleteadmin/@item.ID">Delete</a></td>
                                <td><a href="/Admin/AdminUser/Admindetail/@item.ID">Detail</a></td>
                            </tr>
                            }

                        </tbody>
                    </table>
                </div>

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>



