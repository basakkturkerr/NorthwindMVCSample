
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/SiteLayout.cshtml";
}

@model NorthwindMVCSample.Models.ORM.AdminUser

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Add AdminUser</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Add AdminUser
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <form action="~/admin/adminuser/addadmin/" method="post">
                        <div class="form-group">
                            <label>Name</label>
                            <input type="text" class="form-control" name="Name" value="" />
                        </div>
                        <div class="form-group">
                            <label>Surname</label>
                            <input type="text" class="form-control" name="surname" value="" />
                        </div>
                        <div class="form-group">
                            <label>Email</label>
                            <input type="text" class="form-control" name="email" value="" />
                        </div>
                        <div class="form-group">
                            <label>Password</label>
                            <input type="text" class="form-control" name="password" value="" />
                        </div>
                        <div class="form-group">
                            <input type="submit" class="btn btn-secondary" />
                        </div>
                    </form>
                </div>

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>




