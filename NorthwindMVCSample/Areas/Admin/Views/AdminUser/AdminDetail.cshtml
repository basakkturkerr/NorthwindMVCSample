
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/SiteLayout.cshtml";
}

@model NorthwindMVCSample.Models.ORM.AdminUser

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">AdminUser Detail</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                AdminUser Detail
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <form action="~/admin/adminuser/addadmin/" method="post">
                        <div class="form-group">
                            <label>Name</label>
                            <p>@Model.name</p>
                        </div>
                        <div class="form-group">
                            <label>Surname</label>
                            <p>@Model.surname</p>
                        </div>
                        <div class="form-group">
                            <label>Email</label>
                            <p>@Model.email</p>
                        </div>
                    </form>
                </div>

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
